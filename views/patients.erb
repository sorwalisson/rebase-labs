<h1> Pesquisar Paciente </h1>
<div>
  <form id="patientForm">
    <label for="cpf">Digite o CPF: </label><br>
    <input class="form-control mt-2" type="text" id="cpf" name="cpf" style="width: 18rem;"><br>
    <input type="submit" class="btn btn-outline-primary mb-2">
  </form>
</div>
<div class="container text-center">
  <div class="row">
    <div class="col">
      <div class="container-fluid">
        <div>
          <div class="card" style="width: 18rem;">
            <div class="card-header">
              Dados do Paciente
            </div>
            <ul class="list-group list-group-flush", id="dadospaciente">
          </div>
        </div>
      </div>
    </div>
    <div class="col">
      <div class="container-fluid">
        <div class="container-fluid">
          <div class="card" style="width: 18rem;">
            <div class="card-header">
              Selecione um exame
            </div>
            <div class="dropdown">
              <button class="btn btn-secondary dropdown-toggle" type="button" data-bs-toggle="dropdown" aria-expanded="false">
                Token
              </button>
              <ul class="dropdown-menu" id="droppaciente">
              </ul>
            </div>
          </div>
        </div>
      </div>
    </div>
    <div class="col">
      <div class="container-fluid">
        <div class="container-fluid">
          <div class="card" style="width: 18rem;">
            <div class="card-header">
              Exames
            </div>
            <ul class="list-group list-group-flush" id="exames">
            </ul>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
<script src="patient.js"></script>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js"></script>
